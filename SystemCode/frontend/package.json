{
  "name": "frontend",
  "version": "0.1.0",
  "description": "Front-end Web UI",
  "private": true,
  "dependencies": {
    "@babel/runtime": "7.9.2",
    "@material-ui/core": "4.9.5",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.45",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.1",
    "@testing-library/user-event": "^7.2.1",
    "antd": "4.6.6",
    "autosuggest-highlight": "3.1.1",
    "axios": "0.19.2",
    "chartist": "0.11.4",
    "classnames": "2.2.6",
    "history": "4.10.1",
    "http-proxy-middleware": "^1.0.5",
    "lodash": "^4.17.20",
    "material-ui-dropzone": "3.4.0",
    "node-sass": "4.14.1",
    "perfect-scrollbar": "1.5.0",
    "prop-types": "15.7.2",
    "react": "16.13.0",
    "react-chartist": "0.14.3",
    "react-dom": "16.13.0",
    "react-router-dom": "5.1.2",
    "react-scripts": "^3.4.3",
    "react-swipeable-views": "0.13.9",
    "uuid": "8.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "run-s build:app build:docker",
    "build:app": "react-scripts build",
    "build:docker": "copyfiles -f \"./docker/nginx/*\" ./build && docker build --tag=frontend:dev build",
    "docker:up": "docker-compose -p test -f ./docker/compose.yml up -d",
    "docker:down": "docker-compose -p test -f ./docker/compose.yml down -v"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "copyfiles": "^2.2.0",
    "npm-run-all": "^4.1.5"
  }
}
